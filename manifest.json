{
    "manifest_version": 3,
    "name": "Private Data Blocker",
    "version": "1.0",
    "description": "Manage input field values",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {},
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["panel.css"]
      }
    ]
  }
    
    
    
    